{
  "name": "website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "_generate-supported-browsers": "browserslist --json > src/supportedBrowsers.json",
    "_generate-supported-browsers-regexp": "echo \"const supportedBrowsersRegExp = $(browserslist-useragent-regexp --allowHigherVersions);\n\nexport { supportedBrowsersRegExp };\" > src/supportedBrowsersRegExp.ts",
    "build": "webpack --config webpack.prod.js",
    "build-browser-check": "npm run _generate-supported-browsers && npm run _generate-supported-browsers-regexp && webpack --config webpack.browserCheck.js",
    "dev": "webpack serve --config webpack.dev.js",
    "test": "tsc --noEmit"
  },
  "browserslist": [
    "extends browserslist-config-google",
    "not firefox > 0",
    "not ios_saf > 0",
    "not safari > 0"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.15.0",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@sentry/react": "^6.4.1",
    "@sentry/tracing": "^6.4.1",
    "@types/react-dom": "^17.0.7",
    "@types/uuid": "^8.3.0",
    "@types/wicg-file-system-access": "^2020.9.4",
    "babel-loader": "^8.2.2",
    "browserslist": "^4.16.6",
    "browserslist-config-google": "^3.0.1",
    "browserslist-useragent-regexp": "^3.0.0",
    "clsx": "^1.1.1",
    "copy-webpack-plugin": "^9.0.0",
    "core-js": "^3.19.0",
    "react": "^17.0.2",
    "react-archer": "^3.2.2",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.3",
    "regenerator-runtime": "^0.13.5",
    "ts-loader": "^9.2.6",
    "uuid": "^8.3.2",
    "webpack": "^5.64.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.5.0",
    "webpack-merge": "^5.8.0",
    "workbox-webpack-plugin": "^6.4.2"
  }
}
